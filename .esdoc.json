{
  "source": "./src",
  "includes": ["\\.js$"],
  "destination": "./docs",
  "plugins": [
  { "name": "esdoc-standard-plugin"}, 
  {"name": "esdoc-ecmascript-proposal-plugin", "option": {"all": true}},
  {
  "name": "esdoc-coverage-plugin",
  "option": {
  "enable": true,
  "kind": ["class", "method", "member", "get", "set", "constructor", "function", "variable"]
  }
  }]
 }